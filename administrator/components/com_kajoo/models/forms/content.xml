<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset>

              <field name="id" type="text" default="0" label="COM_KAJOO_FORM_LBL_CONTENT_ID"
                    readonly="true" class="readonly"
                    description="JGLOBAL_FIELD_ID_DESC" /> 

       <field name="added" type="timecreated" default="" 
            label="COM_KAJOO_FORM_LBL_CONTENT_ADDED"
            description="COM_KAJOO_FORM_DESC_CONTENT_ADDED"  /> 

       <field name="updated" type="timeupdated" default="" 
            label="COM_KAJOO_FORM_LBL_CONTENT_UPDATED"
            description="COM_KAJOO_FORM_DESC_CONTENT_UPDATED"  /> 

       <field name="entry_id" type="text" size="40" class="inputbox"
                
            label="COM_KAJOO_FORM_LBL_CONTENT_ENTRY_ID"
            description="COM_KAJOO_FORM_DESC_CONTENT_ENTRY_ID" 
                
                
                 required="true" 
                filter="safehtml" /> 

         <field
                name="state"
                type="list"
                label="JSTATUS"
                description="JFIELD_PUBLISHED_DESC"
                class="inputbox"
                size="1"
                default="1">
                <option value="1">JPUBLISHED</option>
                <option value="0">JUNPUBLISHED</option>
            </field> 

                        <field name="checked_out" type="hidden" filter="unset" />
                <field name="checked_out_time" type="hidden" filter="unset" />

	</fieldset>

</form>
